# include <stdio.h>
# include <stdlib.h>

int check_if_prime (int numberToCheck);

int main () {
  int num = 600851475143;
  int i, primeNum, largest = 0;
  for (i = num; i > 0; i--) {
    if (check_if_prime (i) == 0) {
      printf ("-> %d\n", i);
      break;
    }
  }
  
  return 0;
}


int check_if_prime (int numberToCheck) {
  int i;
  for (i = 2; i < numberToCheck; i++) {
    if (numberToCheck % i == 0) {
      return -1;
    }
  }
  return 0;
}
